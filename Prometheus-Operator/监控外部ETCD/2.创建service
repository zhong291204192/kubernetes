1.kubectl apply -f etcd-prometheus-service.yaml

***********
apiVersion: v1
kind: Service
metadata:
  name: etcd01
  namespace: kube-system
  labels:
    k8s-app: etcd
spec:
  type: ClusterIP
  clusterIP: None
  ports:
  - name: port
    port: 2379
---
apiVersion: v1
kind: Endpoints
metadata:
  name: etcd01
  namespace: kube-system
  labels:
    k8s-app: etcd
subsets:
- addresses:
  - ip: 10.204.80.6
    nodeName: etcd01
  ports:
  - name: port
    port: 2379
---
